from oauth2client.service_account import ServiceAccountCredentials
import pandas as pd
import gspread
import matplotlib
import matplotlib.pyplot as plt

scope = [
'https://www.googleapis.com/auth/spreadsheets',
]

GOOGLE_SHEETS_API_KEY = 'AIzaSyDgQVuZpwLix-kVaV1uNV5rDXa74fihHX0'
credentials = ServiceAccountCredentials.from_json_keyfile_name(GOOGLE_SHEETS_API_KEY, scope)
gc = gspread.authorize(credentials)

sheet_id = "1MQUjcQN1iKwmm-Al67KpCK_GoEygKNB78dx1ml4X1w0"
workbook = gc.open_by_key(sheet_id)
sheet = workbook.get_worksheet(0)
values = sheet.get_all_values()
data = pd.DataFrame(values[1:],columns=values[0])

print(data)